{
  "name": "@r0rri/react-galaxy-bg",
  "version": "2.0.0",
  "type": "module",
  "description": "Un componente React que simula un fondo de galaxia con estrellas animadas y estrellas fugaces",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "react",
    "galaxy",
    "stars",
    "background",
    "animation",
    "space",
    "component",
    "ui"
  ],
  "author": "R0rri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barbarred/react-galaxy-bg.git"
  },
  "bugs": {
    "url": "https://github.com/barbarred/react-galaxy-bg/issues"
  },
  "homepage": "https://github.com/barbarred/react-galaxy-bg#readme",
  "peerDependencies": {
    "react": "16.8.0 || 17.0.0 || 18.0.0 || 19.0.0",
    "react-dom": "16.8.0 || 17.0.0 || 18.0.0 || 19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rollup": "4.53.5",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  }
}
